<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.kteam.lzpt.entity">

	<class name="Project" table="tm_project">
		<id name="id" column="id">
			<generator class="identity" />
		</id>
		
		<property name="year"  length="10"
			type="java.lang.String" />
		
		<property name="createTime" column="createTime" />
		<property name="unit" column="unit" length="255" />
		<property name="createUser" column="createUser" length="255"  />
		<property name="isCheck" column="isCheck" length="11"  />
		<property name="projectType" column="projectType" length="11"  />
		<property name="checkDate" column="checkDate" />
		<property name="score" column="score" />
		<property name="state" column="state" />
		<property name="scoreInfo" column="scoreInfo" length="2000" />
	</class>

</hibernate-mapping>