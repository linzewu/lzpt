<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.kteam.lzpt.entity" >

    <class
        name="WorkExperience"
        table="TM_WorkExperience" >

        <id
            name="id"
            column="c_id"
            type="java.lang.String" >

            <generator class="org.hibernate.id.UUIDGenerator" />
        </id>

        <property
            name="bdate"
            column="c_bdate"
            type="java.util.Date" />

        <property
            name="edate"
            column="c_edate"
            type="java.util.Date" />
            
       <property
            name="wexperise"
            column="c_wexperise"
            type="text" />
       <property name="leaderInfoId" column="c_weid" type="java.lang.String" length="128" />     

    </class>

</hibernate-mapping>