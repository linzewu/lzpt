<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.kteam.lzpt.entity">

	<class name="UnitYearBudget" table="TM_UnitYearBudget">

		<id name="id" column="c_id" type="java.lang.String">
			<generator class="org.hibernate.id.UUIDGenerator" />
		</id>

		<property name="unitId" column="c_unitId" length="120"
			type="java.lang.String" />
		
		<property name="gwjdys" column="c_gwjdys" 
			type="java.lang.Double" />
		
		<property name="ygcgys" column="c_ygcgys" 
			type="java.lang.Double" />
		
		<property name="gwycys" column="c_gwycys" 
			type="java.lang.Double" />
		
		<property name="money" column="c_money" 
			type="java.lang.Double" />
		
		<property name="year" column="c_year" length="100"
			type="java.lang.String" />
		
		<property name="state" column="c_state" 	type="java.lang.Integer" />
		
		<property name="remarks" column="c_remarks" length="4000"
			type="java.lang.String" />

	</class>

</hibernate-mapping>