<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.kteam.lzpt.entity">

	<class name="UnitYearScore" table="TM_UnitYearScore">
		<id name="id" column="c_id" type="java.lang.String">
			<generator class="org.hibernate.id.UUIDGenerator" />
		</id>

		<property name="year" column="c_year" length="100" type="java.lang.String" />

		<property name="unitId" column="c_unitId" length="255" type="java.lang.String" />

		<property name="score" column="c_score" type="java.lang.Double" />
		
	</class>

</hibernate-mapping>