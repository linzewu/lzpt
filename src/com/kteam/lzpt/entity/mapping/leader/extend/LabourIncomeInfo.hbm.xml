<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.kteam.lzpt.entity.leader.extend">

	<class name="LabourIncomeInfo" table="TM_Labour_Income">
		<id name="id" column="c_id" type="java.lang.String">
			<generator class="org.hibernate.id.UUIDGenerator" />
		</id>
		<property name="leaderInfoId" column="c_leaderInfoId" type="java.lang.String" length="128" />
		
		<property name="year" column="c_year" type="java.lang.String" length="100" />
		<property name="total" column="c_total" type="java.lang.Double" />
		<property name="lectures" column="c_lectures" type="java.lang.Double" />
		<property name="writing" column="c_writing" type="java.lang.Double" />
		<property name="consult" column="c_consult" type="java.lang.Double" />
		<property name="copyEdit" column="c_copyEdit" type="java.lang.Double" />
		<property name="painting" column="c_painting" type="java.lang.Double" />
		<property name="other" column="c_other" type="java.lang.Double" />
	</class>

</hibernate-mapping>