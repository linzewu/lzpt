<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.kteam.lzpt.entity" >

    <class
        name="KeyWordsConfig"
        table="TM_KeyWordsConfig" >

        <id
            name="id"
            column="c_id"
            type="java.lang.String" >

            <generator class="org.hibernate.id.UUIDGenerator" />
        </id>

        <property
            name="keyWords"
            column="c_keyWords"
            length="150"
            type="java.lang.String" />

        <bag name="swList" table="TM_KeyWordsConfig_SearchWay" lazy="false"  >
            <key column="c_keywords_id" not-null="true"/>
              <many-to-many class="com.kteam.lzpt.entity.SearchWay">
                  <column name="c_searchWay_id" not-null="true"/>
              </many-to-many>
        </bag>

        <!-- 公用 -->

        <property
            name="createUser"
            column="c_addUser"
            length="50"
            type="java.lang.String" />

        <property
            name="updateUser"
            column="c_updateUser"
            length="50"
            type="java.lang.String" />

        <property
            name="createTime"
            column="c_addTime"
            type="java.util.Date" />

        <property
            name="updateTime"
            column="c_updateTime"
            type="java.util.Date" />

        <property
            name="state"
            column="c_state"
            type="java.lang.Integer" />
    </class>

</hibernate-mapping>