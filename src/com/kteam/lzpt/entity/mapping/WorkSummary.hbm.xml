<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.kteam.lzpt.entity">

	<class name="WorkSummary" table="TM_WorkSummary">

		<id name="id" column="c_id" type="java.lang.String">
			<generator class="org.hibernate.id.UUIDGenerator" />
		</id>


		<property name="years" column="c_years" type="java.lang.Integer" />

		<property name="yearSummary" column="c_yearSummary" type="text" />

		<property name="report" column="c_report" type="text" />

		<property name="jobs" column="c_jobs" type="text" />

	</class>

</hibernate-mapping>